{
  "name": "react-weathr",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "concurrently": "3.0.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "node-sass": "^4.7.2",
    "react-scripts": "^0.6.1"
  },
  "dependencies": {
    "axios": "0.17.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "watch": "concurrently --names 'webpack, node-sass' --prefix name 'npm run start' 'npm run watch:styles'",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "styles": "node-sass src/styles/App.scss --output src/styles --source-map-contents --output-style compressed",
    "watch:styles": "npm run styles -- --watch",
    "eject": "react-scripts eject"
  }
}
